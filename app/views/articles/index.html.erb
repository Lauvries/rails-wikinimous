<h1> All Articles: </h1>


<% @articles.each do |article| %>
<h2><%= link_to (Kramdown::Document.new(article.title).to_html).html_safe, article_path(article) %></h2>
<p><%= (Kramdown::Document.new(article.content).to_html).html_safe %></p>
<% end %>

<h3><%= link_to 'Create new Article', new_article_path(@article) %></h3>
